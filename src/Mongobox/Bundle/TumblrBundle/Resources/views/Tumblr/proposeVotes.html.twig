{% set tumblr_class = 'tumblr-popover' %}
{% set tumblr_id_model = 'tumblr-img-id' %}
{% set tumblr_note_model = 'note-tumblr' %}
{% set tumblr_user_model = 'note-user-tumblr' %}

<legend>Vous devriez noter</legend>
<ul style="list-style:none;">
    {% for tumblr in tumblrs %}
		{% set tumblr = tumblr.tumblr %}
        <li style="display:inline;">
            <img src="{% if tumblr.localPath is null %}{{ tumblr.image }}{% else %}{{ tumblr.localPath }}{% endif %}" alt="{{ tumblr.text }}" title="{{ tumblr.text }}" class="{{ tumblr_class }} {{ tumblr_id_model }}-{{ tumblr.id }}" style="width:110px;height: 90px"> 
        </li>
    {% endfor %}
</ul>